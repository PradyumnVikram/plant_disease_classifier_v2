<Homescreen>:
    MDScrollView:
        do_scroll_x:False
        do_scroll_y:True
        MDBoxLayout:
            id:box
            orientation: 'vertical'
            size_hint_y: 0.9
            pos_hint:{"center_y":0.8}
            padding:10
            spacing:10
            Camera:
                id:camera
                resolution:(640,480)
                play:True
            MDLabel:
                id: x_label
                text: "nothing yet"
                size_hint_y: None
                height: self.texture_size[1]
            MDRaisedButton:
                text:"capture"
                on_release:app.capture_image()
            MDList:
                id:resultbox
           


